                                android:text="@string/service_paper_price_charge"
                                android:textSize="@dimen/txt_size_15"
                                android:textColor="@color/color_262626"
                                android:fontFamily="@font/roboto_medium"/>

                            <RadioButton
                                android:id="@+id/service_paper_reg_rb_price_free"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:paddingStart="@dimen/box_size_6"
                                android:button="@drawable/cb_circle_button"
                                android:buttonTint="@color/color_434f5a"
                                android:text="@string/service_paper_price_free"
                                android:textSize="@dimen/txt_size_15"
                                android:textColor="@color/color_262626"
                                android:fontFamily="@font/roboto_medium"/>
                        </RadioGroup>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/service_paper_reg_ll_menu_device"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/box_size_40"
                    android:layout_marginTop="@dimen/box_size_1"
                    android:paddingStart="@dimen/box_size_15"
