                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:paddingTop="@dimen/box_size_9"
                        android:paddingStart="@dimen/box_size_15"
                        android:paddingEnd="@dimen/box_size_15"
                        android:orientation="vertical"
                        android:background="@color/color_ffffff">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@string/paper_visit_time"
                            android:textSize="@dimen/txt_size_12"
                            android:textColor="@color/color_959595"
                            android:fontFamily="@font/roboto_regular"/>


                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/service_paper_reg_cl_visit_time"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
